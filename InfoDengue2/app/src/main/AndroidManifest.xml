<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.example.android.infodengue"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/mosquito"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".dengue">
        </activity>
        <activity android:name=".Chikungunya">
        </activity>
        <activity android:name=".Zika">
        </activity>
        <activity android:name=".Aedes">
        </activity>
        <activity android:name=".Combate">
        </activity>
    </application>

</manifest>